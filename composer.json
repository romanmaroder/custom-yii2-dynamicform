{
    "name": "romanmaroder/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "romanmaroder",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "minimum-stability":"stable",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/romanmaroder/yii2-dynamicform/issues",
        "wiki": "https://github.com/romanmaroder/yii2-dynamicform/wiki/",
        "source": "https://github.com/romanmaroder/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "romanmaroder",
            "email": "kashik.roman@rambler.ru",
            "homepage": "https://github.com/romanmaroder"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/romanmaroder/yii2-dynamicform"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.5",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/dom-crawler": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "romanmaroder\\dynamicform\\": "src/"
        }
    }
}
